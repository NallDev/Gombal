<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/iv_one"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <Constraint
            android:id="@+id/iv_two"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_editor_absoluteY="0dp"
            motion:layout_editor_absoluteX="0dp"
            android:rotation="90"
            android:translationX="600dp"
            android:translationY="800dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/iv_three"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:rotation="90"
            android:translationX="600dp"
            android:translationY="800dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/v_indicator_one"
            android:layout_width="80dp"
            android:layout_height="8dp"
            android:layout_marginStart="24dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp" >
            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/primary" />
        </Constraint>
        <Constraint
            android:id="@+id/v_indicator_two"
            motion:layout_constraintStart_toEndOf="@id/v_indicator_one"
            android:layout_width="40dp"
            android:layout_height="8dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            android:layout_marginTop="32dp" >
            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/gray" />
        </Constraint>
        <Constraint
            android:id="@+id/v_indicator_three"
            motion:layout_constraintStart_toEndOf="@id/v_indicator_two"
            android:layout_width="40dp"
            android:layout_height="8dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            android:layout_marginTop="32dp" >
            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/gray" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/iv_one"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:rotation="-90"
            android:translationX="-600dp"
            android:translationY="800dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/iv_two"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_editor_absoluteY="0dp"
            motion:layout_editor_absoluteX="0dp"
            android:rotation="0"
            android:translationX="0dp"
            android:translationY="0dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/iv_three"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="invisible"
            android:rotation="90"
            android:translationX="600dp"
            android:translationY="800dp" />
        <Constraint
            android:id="@+id/v_indicator_one"
            android:layout_width="40dp"
            android:layout_height="8dp"
            android:layout_marginStart="24dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp">

            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/gray" />
        </Constraint>
        <Constraint
            android:id="@+id/v_indicator_two"
            motion:layout_constraintStart_toEndOf="@id/v_indicator_one"
            android:layout_width="80dp"
            android:layout_height="8dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            android:layout_marginTop="32dp">

            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/primary" />
        </Constraint>
        <Constraint
            android:id="@+id/v_indicator_three"
            motion:layout_constraintStart_toEndOf="@id/v_indicator_two"
            android:layout_width="40dp"
            android:layout_height="8dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            android:layout_marginTop="32dp">

            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/gray" />
        </Constraint>
    </ConstraintSet>
    <ConstraintSet android:id="@+id/three" >
        <Constraint
            android:id="@+id/iv_one"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_editor_absoluteY="205dp"
            android:rotation="-90"
            android:translationX="-600dp"
            android:translationY="800dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/iv_two"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_editor_absoluteY="0dp"
            motion:layout_editor_absoluteX="0dp"
            android:rotation="-90"
            android:translationX="-600dp"
            android:translationY="800dp"
            android:visibility="invisible" />
        <Constraint
            android:id="@+id/iv_three"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            motion:layout_editor_absoluteY="800dp"
            motion:layout_editor_absoluteX="600dp"
            android:rotation="0"
            android:translationX="0dp"
            android:translationY="0dp"
            android:visibility="visible" />
        <Constraint
            android:id="@+id/v_indicator_one"
            android:layout_width="40dp"
            android:layout_height="8dp"
            android:layout_marginStart="24dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="32dp" >

            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/gray" />
        </Constraint>
        <Constraint
            android:id="@+id/v_indicator_two"
            motion:layout_constraintStart_toEndOf="@id/v_indicator_one"
            android:layout_width="40dp"
            android:layout_height="8dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            android:layout_marginTop="32dp" >

            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/gray" />
        </Constraint>
        <Constraint
            android:id="@+id/v_indicator_three"
            motion:layout_constraintStart_toEndOf="@id/v_indicator_two"
            android:layout_width="80dp"
            android:layout_height="8dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintTop_toTopOf="@id/bg_onboarding"
            android:layout_marginTop="32dp" >

            <CustomAttribute
                motion:attributeName="cardBackgroundColor"
                motion:customColorValue="@color/primary" />
        </Constraint>
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/three"
        motion:duration="1000" />
</MotionScene>